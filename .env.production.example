# Production Environment Configuration
# Copy this file to .env on your server and fill in the actual values

# Bot Configuration
BOT_TOKEN=your_production_bot_token_here

# Database (use existing PostgreSQL server on port 5432)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=sleepbot_db
DB_USER=sleepbot_user
DB_PASSWORD=your_secure_password_here

# Environment
ENVIRONMENT=production

# Logging (info, warning, error only in production)
LOG_LEVEL=INFO

# Timezone
DEFAULT_TIMEZONE=UTC
